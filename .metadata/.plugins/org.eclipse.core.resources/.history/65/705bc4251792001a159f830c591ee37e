package twoSum;

public class TowSum {
	
	
	public TowSum() {
		
	}
	
	public int[] twoSum(int[] nums, int target) {
        int result[] = new int[2];
		int n = 0;
		
		for (int i = 0; i < nums.length; i++) {
			n = target - nums[i];
			result[0] = i;
			for (int j = i + 1; j < nums.length; j++) {
				if (nums[j] == n) {
					 result[1] = j;
					 return result;
				}
			}
		}
		return result;
    }
}
